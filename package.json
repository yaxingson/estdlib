{
  "name": "estdlib",
  "version": "0.1.2",
  "author": "yaxingson <3228891558@qq.com>",
  "description": "a ecmascript standard library",
  "keywords": [
    "util",
    "functional",
    "stdlib"
  ],
  "homepage": "https://github.com/yaxingson/estdlib",
  "repository": {
    "type": "git",
    "url": "git@github.com:yaxingson/estdlib.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*/index.d.ts",
      "import": "./dist/*/index.js",
      "require": "./dist/*/index.cjs"
    }
  },
  "unpkg": "./dist/estdlib.js",
  "jsdelivr": "./dist/estdlib.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "husky",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "docs:build": "typedoc",
    "test": "vitest --coverage",
    "type:check": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier -lw **/*.{ts,js}",
    "bench": "vitest bench",
    "release": "pnpm build && npm publish"
  },
  "files": [
    "dist/*"
  ],
  "engines": {
    "node": ">=16.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.15.0",
    "@material/material-color-utilities": "^0.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^20.12.7",
    "@vitest/coverage-istanbul": "1.6.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "execa": "^8.0.1",
    "globals": "^15.12.0",
    "husky": "^9.0.11",
    "jsdom": "^25.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.16.4",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-license": "^3.5.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typedoc": "0.26.6",
    "typedoc-github-wiki-theme": "^2.1.0",
    "typedoc-plugin-markdown": "4.2.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.16.0",
    "vitest": "^1.5.1"
  },
  "license": "MIT"
}
